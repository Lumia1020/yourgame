<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.lhq.po.Seat" table="T_ZH_SEAT" lazy="true" batch-size="10" dynamic-update="true">
		<id name="uuid" unsaved-value="null">
			<generator class="uuid.hex" />
		</id>
		<property name="seatId" column="SEAT_ID" length="100" />
		<property name="seatName" column="SEAT_NAME" length="100" />
		<property name="stationId" column="STATION_ID" length="1000" />
	</class>
</hibernate-mapping>
