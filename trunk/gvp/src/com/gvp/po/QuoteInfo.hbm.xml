<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gvp.po.QuoteInfo" table="t_quote_info" lazy="true" batch-size="10" dynamic-update="true">
		<id name="qid" column="qid">
			<generator class="native" />
		</id>
		<property name="cid"/>
		<property name="customerName" length="100" />
		<property name="customerType" length="100" />
		<property name="productCode" length="100" />
		<property name="quoter" length="100" />
		<property name="price" length="100"/>
		<property name="recordTime" />
		<property name="modifyTime" />
		<property name="dccNo" />
		<property name="pageNo" />
		<property name="ownerId" />
		<property name="state" length="50"/>
		<property name="adjustDate"/>
	</class>
</hibernate-mapping>
