<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gvp.po.WorkflowLog" table="t_workflow_log" lazy="true" batch-size="10" dynamic-update="true">
		<id name="logid" column="logid">
			<generator class="native" />
		</id>
		<property name="qid" />
		<property name="userid"/>
		<property name="username" length="100"/>
		<property name="description" length="500"/>
		<property name="state" length="50"/>
		<property name="recordTime" />
	</class>
</hibernate-mapping>
