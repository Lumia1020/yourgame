<!DOCTYPE struts PUBLIC '-//Apache Software Foundation//DTD Struts Configuration 2.0//EN'
		'http://struts.apache.org/dtds/struts-2.0.dtd'>
<struts>
	<package name="fly_m-default" extends="struts-default" abstract="true">
		<interceptors>
			<interceptor-stack name="defaultStack">
				<interceptor-ref name="exception"/>
				<interceptor-ref name="alias"/>
				<interceptor-ref name="servletConfig"/>
				<interceptor-ref name="prepare"/>
				<interceptor-ref name="i18n"/>
				<interceptor-ref name="chain"/>
				<interceptor-ref name="debugging"/>
				<interceptor-ref name="profiling"/>
				<interceptor-ref name="scopedModelDriven"/>
				<interceptor-ref name="modelDriven"/>
				<interceptor-ref name="fileUpload"/>
				<interceptor-ref name="checkbox"/>
				<interceptor-ref name="staticParams"/>
				<interceptor-ref name="params">
					<param name="excludeParams">dojo\..*</param>
				</interceptor-ref>
				<interceptor-ref name="conversionError"/>
				<interceptor-ref name="validation">
					<param name="excludeMethods">input,back,cancel,browse</param>
					<param name="validateAnnotatedMethodOnly">true</param>
				</interceptor-ref>
				<interceptor-ref name="workflow">
					<param name="excludeMethods">input,back,cancel,browse</param>
				</interceptor-ref>
			</interceptor-stack>
		</interceptors>
		<default-interceptor-ref name="defaultStack"/>
	</package>
	<package name="article" extends="fly_m-default" namespace="/a">
		<action name="taa" class="articleAction" method="topArticleAjax"/>
		<action name="aaa" class="articleAction" method="auditArticleAjax"/>
		<action name="fdaa" class="articleAction" method="firstDeleteArticleAjax"/>
		<action name="sdaa" class="articleAction" method="shiftDeleteArticleAjax"/>
		<action name="fraa" class="articleAction" method="firstRecoverArticleAjax"/>
		<action name="sa1" class="articleAction" method="saveArticle">
			<interceptor-ref name="defaultStack"/>
			<interceptor-ref name="token"/>
			<result name="invalid.token">/WEB-INF/news/success.jsp</result>
			<result name="ARTICLEACTION_SAVEARTICLE_FAIL">/WEB-INF/news/articleCreate.jsp</result>
			<result name="ARTICLEACTION_SAVEARTICLE_SUCCESS">/WEB-INF/news/success.jsp</result>
			<result name="input">/WEB-INF/news/articleCreate.jsp</result>
		</action>
		<action name="sa2" class="articleAction" method="showArticle">
			<result name="input">/WEB-INF/news/error.jsp</result>
			<result name="ARTICLEACTION_SHOWARTICLE_SUCCESS">/WEB-INF/news/articleShow.jsp</result>
			<result name="ARTICLEACTION_SHOWARTICLE_FAIL">/WEB-INF/news/error.jsp</result>
		</action>
		<action name="qafcf" class="articleAction" method="queryArticleFromCategoryFront">
			<result name="input">/WEB-INF/news/error.jsp</result>
			<result name="ARTICLEACTION_QUERYARTICLEFROMCATEGORYFRONT_SUCCESS">
				/WEB-INF/news/articleShowByCategoryFront.jsp
			</result>
			<result name="ARTICLEACTION_QUERYARTICLEFROMCATEGORYFRONT_FAIL">/WEB-INF/news/error.jsp</result>
		</action>
		<action name="qafcb" class="articleAction" method="queryArticleFromCategoryBack">
			<result name="input">/WEB-INF/news/error.jsp</result>
			<result name="ARTICLEACTION_QUERYARTICLEFROMCATEGORYBACK_SUCCESS">
				/WEB-INF/news/articleShowByCategoryBack.jsp
			</result>
			<result name="ARTICLEACTION_QUERYARTICLEFROMCATEGORYBACK_FAIL">/WEB-INF/news/error.jsp</result>
		</action>
		<action name="aab" class="articleAction" method="auditArticleBatch">
			<interceptor-ref name="defaultStack"/>
			<interceptor-ref name="token"/>
			<result name="invalid.token">/WEB-INF/news/success.jsp</result>
			<result name="ARTICLEACTION_AUDITARTICLEBATCH_SUCCESS">/WEB-INF/news/success.jsp</result>
			<result name="ARTICLEACTION_AUDITARTICLEBATCH_FAIL">/WEB-INF/news/error.jsp</result>
		</action>
		<action name="tab" class="articleAction" method="topArticleBatch">
			<interceptor-ref name="defaultStack"/>
			<interceptor-ref name="token"/>
			<result name="invalid.token">/WEB-INF/news/success.jsp</result>
			<result name="ARTICLEACTION_TOPARTICLEBATCH_SUCCESS">/WEB-INF/news/success.jsp</result>
			<result name="ARTICLEACTION_TOPARTICLEBATCH_FAIL">/WEB-INF/news/error.jsp</result>
		</action>
		<action name="safaatt" class="articleAction" method="showArticleForAddArticleTopType">
			<result name="input">/WEB-INF/news/error.jsp</result>
			<result name="ARTICLEACTION_SHOWARTICLEFORADDARTICLETOPTYPE_SUCCESS">
				/WEB-INF/news/articleAddArticleTopType.jsp
			</result>
			<result name="ARTICLEACTION_SHOWARTICLEFORADDARTICLETOPTYPE_FAIL">/WEB-INF/news/error.jsp</result>
		</action>
		<action name="eav" class="articleAction" method="editArticleView">
			<result name="input">/WEB-INF/news/articleEdit.jsp</result>
			<result name="ARTICLEACTION_EDITARTICLEVIEW_SUCCESS">/WEB-INF/news/articleEdit.jsp</result>
			<result name="ARTICLEACTION_EDITARTICLEVIEW_FAIL">/WEB-INF/news/error.jsp</result>
		</action>
		<action name="ta" class="articleAction" method="topArticle">
			<interceptor-ref name="defaultStack"/>
			<interceptor-ref name="token"/>
			<result name="input">/WEB-INF/news/error.jsp</result>
			<result name="invalid.token">/WEB-INF/news/success.jsp</result>
			<result name="ARTICLEACTION_TOPARTICLE_SUCCESS">/WEB-INF/news/success.jsp</result>
			<result name="ARTICLEACTION_TOPARTICLE_FAIL">/WEB-INF/news/error.jsp</result>
		</action>
		<action name="eas" class="articleAction" method="editArticleSave">
			<interceptor-ref name="defaultStack"/>
			<interceptor-ref name="token"/>
			<result name="input">/WEB-INF/news/error.jsp</result>
			<result name="invalid.token">/WEB-INF/news/success.jsp</result>
			<result name="ARTICLEACTION_EDITARTICLESAVE_SUCCESS">/WEB-INF/news/success.jsp</result>
			<result name="ARTICLEACTION_EDITARTICLESAVE_FAIL">/WEB-INF/news/articleEdit.jsp</result>
		</action>
		<action name="qafdb" class="articleAction" method="queryArticleFirstDeletedBack">
			<result name="ARTICLEACTION_QUERYARTICLEFIRSTDELETEDBACK_SUCCESS">
				/WEB-INF/news/articleListByFirstDeleted.jsp
			</result>
			<result name="ARTICLEACTION_QUERYARTICLEFIRSTDELETEDBACK_FAIL">/WEB-INF/news/error.jsp</result>
		</action>
		<action name="qancb" class="articleAction" method="queryArticleNoCategoryBack">
			<result name="ARTICLEACTION_QUERYARTICLENOCATEGORYBACK_SUCCESS">/WEB-INF/news/articleListByNoCategory.jsp
			</result>
			<result name="ARTICLEACTION_QUERYARTICLENOCATEGORYBACK_FAIL">/WEB-INF/news/error.jsp</result>
		</action>
		<action name="cacb" class="articleAction" method="changeArticleCategoryBatch">
			<interceptor-ref name="defaultStack"/>
			<interceptor-ref name="token"/>
			<result name="input">/WEB-INF/news/error.jsp</result>
			<result name="invalid.token">/WEB-INF/news/success.jsp</result>
			<result name="ARTICLEACTION_CHANGEARTICLECATEGORYBATCH_SUCCESS">/WEB-INF/news/success.jsp</result>
			<result name="ARTICLEACTION_CHANGEARTICLECATEGORYBATCH_FAIL">/WEB-INF/news/error.jsp</result>
		</action>
		<action name="qabd" class="articleAction" method="queryArticleByDynamik">
			<result name="ARTICLEACTION_QUERYARTICLEBYDYNAMIC_SUCCESS">/WEB-INF/news/articleListByDynamik.jsp</result>
			<result name="ARTICLEACTION_QUERYARTICLEBYDYNAMIC_FAIL">/WEB-INF/news/error.jsp</result>
		</action>
		<action name="*">
			<result>/WEB-INF/news/{1}.jsp</result>
		</action>
	</package>

	<package name="category" extends="fly_m-default" namespace="/c">
		<action name="stcla" class="categoryAction" method="showTopCategoryListAjax"/>
		<action name="scla" class="categoryAction" method="showCategoryListAjax"/>
		<action name="scla2" class="categoryAction" method="showCategoryListAjax2"/>
		<action name="sccla" class="categoryAction" method="showCategoryChainListAjax"/>
		<action name="sc" class="categoryAction" method="saveCategory">
			<interceptor-ref name="token"/>
			<interceptor-ref name="defaultStack"/>
			<result name="invalid.token">/WEB-INF/news/success.jsp</result>
			<result name="input">/WEB-INF/news/categoryCreate.jsp</result>
			<result name="CATEGORYACTION_SAVECATEGORY_FAIL">/WEB-INF/news/categoryCreate.jsp</result>
			<result name="CATEGORYACTION_SAVECATEGORY_SUCCESS">/WEB-INF/news/success.jsp</result>
		</action>
		<action name="sc1" class="categoryAction" method="showCategory">
			<result name="input">/WEB-INF/news/error.jsp</result>
			<result name="CATEGORYACTION_SHOWCATEGORY_FAIL">/WEB-INF/news/error.jsp</result>
			<result name="CATEGORYACTION_SHOWCATEGORY_SUCCESS">/WEB-INF/news/categoryShow.jsp</result>
		</action>
		<action name="sc2" class="categoryAction" method="showCategory">
			<result name="input">/WEB-INF/news/error.jsp</result>
			<result name="CATEGORYACTION_SHOWCATEGORY_FAIL">/WEB-INF/news/error.jsp</result>
			<result name="CATEGORYACTION_SHOWCATEGORY_SUCCESS">/WEB-INF/news/categoryUpdate.jsp</result>
		</action>
		<action name="dc" class="categoryAction" method="deleteCategory">
			<result name="input">/WEB-INF/news/error.jsp</result>
			<result name="CATEGORYACTION_DELETECATEGORY_SUCCESS">/WEB-INF/news/success.jsp</result>
		</action>
		<action name="uc" class="categoryAction" method="updateCategory">
			<interceptor-ref name="token"/>
			<interceptor-ref name="defaultStack"/>
			<result name="invalid.token">/WEB-INF/news/success.jsp</result>
			<result name="input">/WEB-INF/news/categoryUpdate.jsp</result>
			<result name="CATEGORYACTION_UPDATECATEGORY_FAIL">/WEB-INF/news/categoryUpdate.jsp</result>
			<result name="CATEGORYACTION_UPDATECATEGORY_SUCCESS">/WEB-INF/news/success.jsp</result>
		</action>
		<action name="*">
			<result>/WEB-INF/news/{1}.jsp</result>
		</action>
	</package>
</struts>
