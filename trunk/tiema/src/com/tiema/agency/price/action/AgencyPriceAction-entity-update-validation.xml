<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
	<field name="agencyPrice">
		<field-validator type="visitor">
			<param name="context">context</param>
			<param name="appendPrefix">true</param>
			<message></message>
		</field-validator>
	</field>
	<field name="agencyPrice.agencyCompany.id">
		<field-validator type="required">
			<message><![CDATA[找不到中介公司的记录ID!]]></message>
		</field-validator>
	</field>
	<field name="agencyPrice.endDate">
		<field-validator type="agencyPriceDateRangeValid">
			<param name="update">true</param>
			<param name="startDateFieldName">agencyPrice.startDate</param>
			<param name="endDateFieldName">agencyPrice.endDate</param>
			<param name="agencyPriceIdFieldName">agencyPrice.id</param>
			<param name="agencyCompanyIdFieldName">agencyPrice.agencyCompany.id</param>
			<message><![CDATA[<b>${agencyPrice.startDate}</b> 到 <b>${agencyPrice.endDate}</b> 的日期范围不可用，请更换其他日期范围。 ]]></message>
		</field-validator>
	</field>
</validators>
