<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="default-agency-company" namespace="/default/agency-company" extends="struts-default">
		<action name="*">
			<result>/module/{1}/{1}_manage.jsp</result>
		</action>
	</package>

	<package name="json-agency-company" namespace="/json/agency-company" extends="json-default">
		<action name="page!*" class="agencyCompanyAction">
			<result type="json">
				<param name="enumAsBean">true</param>
				<param name="root">page</param>
				<param name="includeProperties"><![CDATA[
					result.*,
					totalCount,
					success
				]]></param>
			</result>
		</action>
		<action name="success!*" class="agencyCompanyAction">
			<result type="json">
				<param name="enumAsBean">true</param>
				<param name="includeProperties">success,jsons.*,agencyCompany\..*</param>
			</result>
		</action>


		<action name="entity-*" class="agencyCompanyAction" method="{1}">
			<interceptor-ref name="jsonValidationWorkflowStack" />
			<result type="json">
				<param name="enumAsBean">true</param>
				<param name="includeProperties">agencyCompany\..*,success,jsons.*</param>
			</result>
		</action>
	</package>
</struts>
