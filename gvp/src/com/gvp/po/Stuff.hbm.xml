<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gvp.po.Stuff" table="t_stuff" lazy="true" batch-size="10" dynamic-update="true">
		<id name="stuffid" column="stuffid">
			<generator class="native" />
		</id>
		<property name="stuffName" length="200" />
		<!--<many-to-one name="provider" column="providerid" class="com.gvp.po.Provider" insert="true" update="true"></many-to-one>-->
	</class>
</hibernate-mapping>
