<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gvp.po.ProcessInfo" table="t_process_info" lazy="true" batch-size="10" dynamic-update="true">
		<id name="pid" column="pid">
			<generator class="native" />
		</id>
		<property name="models" length="100" />
		<property name="processName" length="100" />
		<property name="dayOutput" length="100" />
		<property name="processTime" length="100" />
		<property name="attritionRate" />
		<property name="processPrice" length="100" />
		<property name="conditioners" length="100" />
		<property name="qid" />
	</class>
</hibernate-mapping>
